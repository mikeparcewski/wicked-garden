{
  "catalogId": "cache",
  "version": "2.0.0",
  "description": "Cache management components for monitoring, configuration, warming, and lifecycle control",
  "components": {
    "CacheDashboard": {
      "description": "Main cache management dashboard with overview and actions.",
      "category": "layout",
      "props": {
        "title": {"type": "string", "description": "Dashboard title", "default": "Cache Management"},
        "refreshInterval": {"type": "number", "description": "Auto-refresh interval in seconds"}
      },
      "slots": {
        "overview": {"role": "display", "description": "Overview metrics", "accepts": ["CacheOverview", "CacheHealth"]},
        "namespaces": {"role": "content", "description": "Namespace cards", "accepts": ["CacheNamespace"]},
        "actions": {"role": "control", "description": "Management actions", "accepts": ["CacheAction"]}
      }
    },
    "CacheOverview": {
      "description": "Overview of all cache namespaces with size and hit rates.",
      "category": "display",
      "props": {
        "totalSize": {"type": "number", "description": "Total cache size in bytes"},
        "sizeLimit": {"type": "number", "description": "Max size limit in bytes"},
        "namespaceCount": {"type": "number", "description": "Number of namespaces"},
        "totalEntries": {"type": "number", "description": "Total entries across all namespaces"},
        "overallHitRate": {"type": "number", "description": "Overall cache hit rate (0-1)"},
        "uptime": {"type": "string", "description": "Time since last cache clear"}
      }
    },
    "CacheHealth": {
      "description": "Overall cache health indicators.",
      "category": "display",
      "props": {
        "status": {"type": "string", "description": "Health status", "enum": ["healthy", "warning", "critical"], "required": true},
        "hitRateStatus": {"type": "string", "description": "Hit rate health", "enum": ["good", "degraded", "poor"]},
        "sizeStatus": {"type": "string", "description": "Size health", "enum": ["ok", "high", "critical"]},
        "evictionRate": {"type": "number", "description": "Recent eviction rate"},
        "errorCount": {"type": "number", "description": "Recent errors"},
        "recommendations": {"type": "array", "description": "Health recommendations"}
      }
    },
    "CacheNamespace": {
      "description": "A single cache namespace with detailed stats.",
      "category": "content",
      "props": {
        "name": {"type": "string", "description": "Namespace name", "required": true},
        "description": {"type": "string", "description": "Namespace purpose"},
        "entryCount": {"type": "number", "description": "Number of entries"},
        "size": {"type": "number", "description": "Size in bytes"},
        "sizeLimit": {"type": "number", "description": "Namespace size limit"},
        "hitRate": {"type": "number", "description": "Hit rate (0-1)"},
        "hits": {"type": "number", "description": "Total hits"},
        "misses": {"type": "number", "description": "Total misses"},
        "ttl": {"type": "number", "description": "Default TTL in seconds"},
        "lastAccess": {"type": "string", "description": "Last access timestamp"},
        "createdAt": {"type": "string", "description": "When namespace was created"},
        "status": {"type": "string", "description": "Namespace status", "enum": ["active", "warming", "stale", "disabled"]}
      }
    },
    "CacheEntry": {
      "description": "A single cache entry with full metadata.",
      "category": "content",
      "props": {
        "key": {"type": "string", "description": "Cache key", "required": true},
        "namespace": {"type": "string", "description": "Parent namespace"},
        "valuePreview": {"type": "string", "description": "Truncated value preview"},
        "valueType": {"type": "string", "description": "Value type", "enum": ["string", "json", "binary", "compressed"]},
        "size": {"type": "number", "description": "Entry size in bytes"},
        "createdAt": {"type": "string", "description": "Creation timestamp"},
        "expiresAt": {"type": "string", "description": "Expiration timestamp"},
        "ttl": {"type": "number", "description": "Time to live remaining (seconds)"},
        "accessCount": {"type": "number", "description": "Times accessed"},
        "lastAccess": {"type": "string", "description": "Last access time"},
        "tags": {"type": "array", "description": "Entry tags for grouping"},
        "priority": {"type": "string", "description": "Eviction priority", "enum": ["high", "normal", "low"]}
      }
    },
    "CacheEntryList": {
      "description": "Browsable list of cache entries with filtering.",
      "category": "layout",
      "props": {
        "namespace": {"type": "string", "description": "Namespace to show entries from"},
        "sortBy": {"type": "string", "description": "Sort field", "enum": ["key", "size", "ttl", "accessCount", "createdAt"]},
        "sortOrder": {"type": "string", "description": "Sort direction", "enum": ["asc", "desc"]},
        "filter": {"type": "string", "description": "Key filter pattern"}
      },
      "slots": {
        "entries": {"role": "content", "description": "Cache entries", "accepts": ["CacheEntry"]}
      }
    },
    "CacheStats": {
      "description": "Detailed cache performance statistics.",
      "category": "display",
      "props": {
        "namespace": {"type": "string", "description": "Namespace (or 'all')"},
        "timeRange": {"type": "string", "description": "Stats time range", "enum": ["1h", "24h", "7d", "30d"]},
        "hits": {"type": "number", "description": "Cache hits"},
        "misses": {"type": "number", "description": "Cache misses"},
        "hitRate": {"type": "number", "description": "Hit rate (0-1)"},
        "evictions": {"type": "number", "description": "Entries evicted"},
        "expirations": {"type": "number", "description": "Entries expired"},
        "avgLatency": {"type": "number", "description": "Average lookup latency (ms)"},
        "p99Latency": {"type": "number", "description": "P99 latency (ms)"},
        "bytesRead": {"type": "number", "description": "Total bytes read"},
        "bytesWritten": {"type": "number", "description": "Total bytes written"}
      }
    },
    "CacheStatsChart": {
      "description": "Time-series chart of cache metrics.",
      "category": "display",
      "props": {
        "metric": {"type": "string", "description": "Metric to chart", "enum": ["hitRate", "size", "entries", "latency", "evictions"], "required": true},
        "namespace": {"type": "string", "description": "Namespace (or 'all')"},
        "timeRange": {"type": "string", "description": "Time range", "enum": ["1h", "24h", "7d", "30d"]},
        "data": {"type": "array", "description": "Chart data points"}
      }
    },
    "CachePolicy": {
      "description": "Cache policy configuration for a namespace.",
      "category": "content",
      "props": {
        "namespace": {"type": "string", "description": "Namespace name", "required": true},
        "defaultTTL": {"type": "number", "description": "Default TTL in seconds"},
        "maxTTL": {"type": "number", "description": "Maximum allowed TTL"},
        "maxSize": {"type": "number", "description": "Max namespace size in bytes"},
        "maxEntries": {"type": "number", "description": "Max number of entries"},
        "evictionPolicy": {"type": "string", "description": "Eviction strategy", "enum": ["lru", "lfu", "fifo", "ttl", "priority"]},
        "compressionEnabled": {"type": "boolean", "description": "Compress large values"},
        "compressionThreshold": {"type": "number", "description": "Min size to compress (bytes)"}
      }
    },
    "PolicyEditor": {
      "description": "Form for editing cache policies.",
      "category": "control",
      "props": {
        "namespace": {"type": "string", "description": "Namespace to configure", "required": true},
        "currentPolicy": {"type": "object", "description": "Current policy settings"}
      }
    },
    "CacheWarmingStatus": {
      "description": "Status of cache warming operations.",
      "category": "display",
      "props": {
        "status": {"type": "string", "description": "Warming status", "enum": ["idle", "running", "scheduled", "failed"], "required": true},
        "progress": {"type": "number", "description": "Progress percentage (0-100)"},
        "itemsWarmed": {"type": "number", "description": "Items warmed so far"},
        "totalItems": {"type": "number", "description": "Total items to warm"},
        "startedAt": {"type": "string", "description": "When warming started"},
        "estimatedCompletion": {"type": "string", "description": "Estimated completion time"},
        "errors": {"type": "array", "description": "Warming errors"}
      },
      "slots": {
        "tasks": {"role": "content", "description": "Warming tasks", "accepts": ["WarmingTask"]}
      }
    },
    "WarmingTask": {
      "description": "A specific cache warming task.",
      "category": "content",
      "props": {
        "id": {"type": "string", "description": "Task ID", "required": true},
        "name": {"type": "string", "description": "Task name", "required": true},
        "namespace": {"type": "string", "description": "Target namespace"},
        "source": {"type": "string", "description": "Data source", "enum": ["file", "api", "database", "computation"]},
        "status": {"type": "string", "description": "Task status", "enum": ["pending", "running", "completed", "failed"]},
        "itemCount": {"type": "number", "description": "Items to warm"},
        "progress": {"type": "number", "description": "Progress percentage"},
        "schedule": {"type": "string", "description": "Cron schedule if recurring"}
      }
    },
    "WarmingScheduler": {
      "description": "Configure scheduled cache warming.",
      "category": "control",
      "props": {
        "namespace": {"type": "string", "description": "Target namespace"},
        "schedules": {"type": "array", "description": "Configured warming schedules"}
      }
    },
    "CacheDependencyGraph": {
      "description": "Visualize dependencies between cached items.",
      "category": "display",
      "props": {
        "namespace": {"type": "string", "description": "Namespace to visualize"},
        "nodes": {"type": "array", "description": "Cache entry nodes"},
        "edges": {"type": "array", "description": "Dependency edges"},
        "layout": {"type": "string", "description": "Graph layout", "enum": ["hierarchical", "force", "radial"]}
      }
    },
    "EvictionQueue": {
      "description": "Show items about to be evicted.",
      "category": "layout",
      "props": {
        "namespace": {"type": "string", "description": "Namespace"},
        "limit": {"type": "number", "description": "Max items to show", "default": 20},
        "reason": {"type": "string", "description": "Filter by eviction reason", "enum": ["ttl", "size", "lru", "manual"]}
      },
      "slots": {
        "entries": {"role": "content", "description": "Entries to be evicted", "accepts": ["CacheEntry"]}
      }
    },
    "CacheAction": {
      "description": "Action button for cache operations.",
      "category": "control",
      "props": {
        "action": {"type": "string", "description": "Action type", "enum": ["clear", "clear_namespace", "refresh", "export", "import", "warm", "disable", "enable"], "required": true},
        "namespace": {"type": "string", "description": "Target namespace (or 'all')"},
        "label": {"type": "string", "description": "Button label"},
        "confirmRequired": {"type": "boolean", "description": "Require confirmation", "default": true},
        "dangerous": {"type": "boolean", "description": "Mark as dangerous action", "default": false}
      }
    },
    "BulkOperationPanel": {
      "description": "Panel for bulk cache operations.",
      "category": "layout",
      "props": {
        "namespace": {"type": "string", "description": "Target namespace"},
        "selectedKeys": {"type": "array", "description": "Selected cache keys"}
      },
      "slots": {
        "actions": {"role": "control", "description": "Bulk actions", "accepts": ["CacheAction"]}
      }
    },
    "InvalidationLog": {
      "description": "Log of cache invalidation events.",
      "category": "layout",
      "props": {
        "title": {"type": "string", "description": "Log title", "default": "Invalidation Log"},
        "limit": {"type": "number", "description": "Max events to show", "default": 50},
        "namespace": {"type": "string", "description": "Filter by namespace"},
        "trigger": {"type": "string", "description": "Filter by trigger type"}
      },
      "slots": {
        "events": {"role": "content", "description": "Invalidation events", "accepts": ["InvalidationEvent"]}
      }
    },
    "InvalidationEvent": {
      "description": "A cache invalidation event.",
      "category": "content",
      "props": {
        "id": {"type": "string", "description": "Event ID"},
        "timestamp": {"type": "string", "description": "Event timestamp", "required": true},
        "namespace": {"type": "string", "description": "Affected namespace"},
        "trigger": {"type": "string", "description": "What triggered invalidation", "enum": ["ttl", "manual", "file_change", "dependency", "policy", "error"]},
        "keysAffected": {"type": "number", "description": "Number of keys invalidated"},
        "keys": {"type": "array", "description": "Specific keys invalidated"},
        "reason": {"type": "string", "description": "Invalidation reason"},
        "initiator": {"type": "string", "description": "Who/what initiated"}
      }
    },
    "CacheTimeline": {
      "description": "Timeline view of cache events.",
      "category": "display",
      "props": {
        "namespace": {"type": "string", "description": "Filter by namespace"},
        "timeRange": {"type": "string", "description": "Time range to show"},
        "eventTypes": {"type": "array", "description": "Event types to include", "default": ["write", "read", "evict", "expire", "warm"]}
      }
    },
    "SizeQuotaIndicator": {
      "description": "Visual indicator of cache size vs quota.",
      "category": "display",
      "props": {
        "namespace": {"type": "string", "description": "Namespace (or 'all')"},
        "currentSize": {"type": "number", "description": "Current size in bytes", "required": true},
        "quota": {"type": "number", "description": "Size quota in bytes", "required": true},
        "warningThreshold": {"type": "number", "description": "Warning at % of quota", "default": 80},
        "criticalThreshold": {"type": "number", "description": "Critical at % of quota", "default": 95}
      }
    },
    "CacheComparison": {
      "description": "Compare metrics between namespaces.",
      "category": "display",
      "props": {
        "namespaces": {"type": "array", "description": "Namespaces to compare", "required": true},
        "metrics": {"type": "array", "description": "Metrics to compare", "default": ["hitRate", "size", "entries", "latency"]}
      }
    }
  },
  "intents": {
    "show-cache-dashboard": {
      "description": "Show main cache management dashboard",
      "suggestedComponents": ["CacheDashboard", "CacheOverview", "CacheHealth", "CacheNamespace"],
      "requiredData": ["cacheStats"]
    },
    "show-cache-overview": {
      "description": "Show overview of all caches",
      "suggestedComponents": ["CacheOverview", "CacheNamespace", "SizeQuotaIndicator"],
      "requiredData": ["cacheStats"]
    },
    "show-cache-health": {
      "description": "Show cache health and recommendations",
      "suggestedComponents": ["CacheHealth", "CacheStats"],
      "requiredData": ["cacheHealth"]
    },
    "show-cache-stats": {
      "description": "Show detailed cache performance statistics",
      "suggestedComponents": ["CacheStats", "CacheStatsChart"],
      "requiredData": ["cacheStats"]
    },
    "show-cache-entries": {
      "description": "Browse cache entries for a namespace",
      "suggestedComponents": ["CacheEntryList", "CacheEntry"],
      "requiredData": ["cacheEntries"]
    },
    "show-cache-policy": {
      "description": "Show and edit cache policies",
      "suggestedComponents": ["CachePolicy", "PolicyEditor"],
      "requiredData": ["cachePolicy"]
    },
    "show-warming-status": {
      "description": "Show cache warming status and tasks",
      "suggestedComponents": ["CacheWarmingStatus", "WarmingTask", "WarmingScheduler"],
      "requiredData": ["warmingStatus"]
    },
    "show-eviction-queue": {
      "description": "Show items pending eviction",
      "suggestedComponents": ["EvictionQueue", "CacheEntry"],
      "requiredData": ["evictionQueue"]
    },
    "show-invalidation-log": {
      "description": "Show cache invalidation history",
      "suggestedComponents": ["InvalidationLog", "InvalidationEvent", "CacheTimeline"],
      "requiredData": ["invalidationLog"]
    },
    "show-cache-dependencies": {
      "description": "Show cache dependency graph",
      "suggestedComponents": ["CacheDependencyGraph"],
      "requiredData": ["cacheDependencies"]
    },
    "manage-cache": {
      "description": "Cache management actions (clear, warm, export)",
      "suggestedComponents": ["CacheDashboard", "CacheAction", "BulkOperationPanel"],
      "requiredData": ["cacheStats"]
    },
    "compare-namespaces": {
      "description": "Compare cache namespaces",
      "suggestedComponents": ["CacheComparison", "CacheNamespace"],
      "requiredData": ["cacheStats"]
    }
  },
  "dataBindings": {
    "cacheStats": {
      "mcpServer": "cache",
      "tool": "stats",
      "description": "Cache statistics"
    },
    "cacheHealth": {
      "mcpServer": "cache",
      "tool": "health",
      "description": "Cache health status"
    },
    "cacheEntries": {
      "mcpServer": "cache",
      "tool": "list",
      "description": "Cache entries"
    },
    "cachePolicy": {
      "mcpServer": "cache",
      "tool": "get_policy",
      "description": "Cache policy configuration"
    },
    "warmingStatus": {
      "mcpServer": "cache",
      "tool": "warming_status",
      "description": "Warming operation status"
    },
    "evictionQueue": {
      "mcpServer": "cache",
      "tool": "eviction_queue",
      "description": "Pending evictions"
    },
    "invalidationLog": {
      "mcpServer": "cache",
      "tool": "invalidation_log",
      "description": "Invalidation events"
    },
    "cacheDependencies": {
      "mcpServer": "cache",
      "tool": "dependencies",
      "description": "Cache dependency graph"
    }
  }
}
