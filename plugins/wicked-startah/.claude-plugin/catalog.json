{
  "catalogId": "startah",
  "version": "1.0.0",
  "description": "Starter kit components including browser automation, multi-AI collaboration, and capability discovery",
  "components": {
    "BrowserDashboard": {
      "description": "Main browser automation dashboard showing session status and results.",
      "category": "layout",
      "props": {
        "title": {"type": "string", "description": "Dashboard title", "default": "Browser Automation"},
        "sessionActive": {"type": "boolean", "description": "Whether a browser session is active"},
        "currentUrl": {"type": "string", "description": "Currently open URL"}
      },
      "slots": {
        "screenshots": {"role": "content", "description": "Screenshot gallery", "accepts": ["ScreenshotCard", "ScreenshotGrid"]},
        "results": {"role": "content", "description": "Test/audit results", "accepts": ["A11yAuditResult", "TestResult"]},
        "actions": {"role": "content", "description": "Action log", "accepts": ["ActionLog", "ActionItem"]}
      }
    },
    "ScreenshotCard": {
      "description": "Display a captured screenshot with metadata.",
      "category": "display",
      "props": {
        "src": {"type": "string", "description": "Screenshot image path or URL", "required": true},
        "title": {"type": "string", "description": "Screenshot title"},
        "url": {"type": "string", "description": "URL when screenshot was taken"},
        "timestamp": {"type": "string", "description": "When screenshot was captured"},
        "viewport": {"type": "string", "description": "Viewport size (e.g., '1920x1080')"}
      }
    },
    "ScreenshotGrid": {
      "description": "Grid layout for multiple screenshots.",
      "category": "layout",
      "props": {
        "columns": {"type": "number", "description": "Number of columns", "default": 3}
      },
      "slots": {
        "children": {"role": "content", "accepts": ["ScreenshotCard"]}
      }
    },
    "A11yAuditResult": {
      "description": "Accessibility audit results with violations and passes.",
      "category": "display",
      "props": {
        "url": {"type": "string", "description": "Audited URL"},
        "score": {"type": "number", "description": "Accessibility score (0-100)"},
        "violations": {"type": "number", "description": "Number of violations"},
        "passes": {"type": "number", "description": "Number of passed checks"},
        "timestamp": {"type": "string", "description": "When audit was run"}
      },
      "slots": {
        "violations": {"role": "content", "accepts": ["A11yViolation"]}
      }
    },
    "A11yViolation": {
      "description": "Individual accessibility violation.",
      "category": "content",
      "props": {
        "id": {"type": "string", "description": "Violation rule ID"},
        "impact": {"type": "string", "description": "Impact level", "enum": ["critical", "serious", "moderate", "minor"]},
        "description": {"type": "string", "description": "What the violation is"},
        "help": {"type": "string", "description": "How to fix it"},
        "nodes": {"type": "number", "description": "Number of affected elements"}
      }
    },
    "TestResult": {
      "description": "Browser test execution result.",
      "category": "display",
      "props": {
        "name": {"type": "string", "description": "Test name", "required": true},
        "status": {"type": "string", "description": "Test status", "enum": ["passed", "failed", "skipped", "pending"]},
        "duration": {"type": "number", "description": "Duration in milliseconds"},
        "error": {"type": "string", "description": "Error message if failed"},
        "screenshot": {"type": "string", "description": "Screenshot path if captured"}
      }
    },
    "ActionLog": {
      "description": "Log of browser automation actions.",
      "category": "layout",
      "props": {
        "title": {"type": "string", "description": "Log title", "default": "Action History"}
      },
      "slots": {
        "children": {"role": "content", "accepts": ["ActionItem"]}
      }
    },
    "ActionItem": {
      "description": "Single browser automation action.",
      "category": "content",
      "props": {
        "action": {"type": "string", "description": "Action type (open, click, fill, etc.)", "required": true},
        "target": {"type": "string", "description": "Target URL or selector"},
        "value": {"type": "string", "description": "Value if applicable"},
        "status": {"type": "string", "description": "Action status", "enum": ["success", "failed", "pending"]},
        "timestamp": {"type": "string", "description": "When action was executed"}
      }
    },
    "PageSnapshot": {
      "description": "Page content snapshot with accessibility tree.",
      "category": "display",
      "props": {
        "url": {"type": "string", "description": "Page URL"},
        "title": {"type": "string", "description": "Page title"},
        "textContent": {"type": "string", "description": "Extracted text content"},
        "links": {"type": "number", "description": "Number of links found"},
        "forms": {"type": "number", "description": "Number of forms found"},
        "images": {"type": "number", "description": "Number of images found"}
      }
    },
    "ViewportPreview": {
      "description": "Live or captured viewport preview.",
      "category": "display",
      "props": {
        "src": {"type": "string", "description": "Image source or WebSocket URL"},
        "width": {"type": "number", "description": "Viewport width"},
        "height": {"type": "number", "description": "Viewport height"},
        "device": {"type": "string", "description": "Device name if emulating"},
        "live": {"type": "boolean", "description": "Whether streaming live", "default": false}
      }
    },
    "DeviceSelector": {
      "description": "Device emulation selector.",
      "category": "input",
      "props": {
        "selected": {"type": "string", "description": "Currently selected device"},
        "devices": {"type": "array", "description": "Available devices"}
      }
    },
    "AIConversationPanel": {
      "description": "Multi-AI conversation view showing perspectives from different models.",
      "category": "layout",
      "props": {
        "title": {"type": "string", "description": "Conversation topic"},
        "models": {"type": "array", "description": "AI models involved"}
      },
      "slots": {
        "perspectives": {"role": "content", "accepts": ["AIPerspective"]},
        "synthesis": {"role": "content", "accepts": ["ConsensusSummary"]}
      }
    },
    "AIPerspective": {
      "description": "Single AI model's perspective on a topic.",
      "category": "content",
      "props": {
        "model": {"type": "string", "description": "AI model name (Claude, Gemini, etc.)", "required": true},
        "content": {"type": "string", "description": "Model's response"},
        "timestamp": {"type": "string", "description": "When response was generated"},
        "confidence": {"type": "string", "description": "Confidence level", "enum": ["high", "medium", "low"]}
      }
    },
    "ConsensusSummary": {
      "description": "Summary of multi-AI consensus.",
      "category": "display",
      "props": {
        "agreements": {"type": "array", "description": "Points all models agreed on"},
        "disagreements": {"type": "array", "description": "Points of disagreement"},
        "uniqueInsights": {"type": "array", "description": "Unique insights from individual models"},
        "recommendation": {"type": "string", "description": "Final recommendation"}
      }
    },
    "CapabilityCard": {
      "description": "Display an available capability (MCP, skill, or agent).",
      "category": "display",
      "props": {
        "name": {"type": "string", "description": "Capability name", "required": true},
        "type": {"type": "string", "description": "Capability type", "enum": ["mcp", "skill", "agent"]},
        "description": {"type": "string", "description": "What it does"},
        "status": {"type": "string", "description": "Availability", "enum": ["available", "unavailable", "recommended"]}
      }
    },
    "CapabilityGrid": {
      "description": "Grid of discovered capabilities.",
      "category": "layout",
      "slots": {
        "children": {"role": "content", "accepts": ["CapabilityCard"]}
      }
    }
  },
  "intents": {
    "browser-results": {
      "description": "Show browser automation results",
      "suggestedComponents": ["BrowserDashboard", "ScreenshotGrid", "A11yAuditResult"]
    },
    "accessibility-audit": {
      "description": "Show accessibility audit results",
      "suggestedComponents": ["A11yAuditResult", "A11yViolation"]
    },
    "multi-ai-discussion": {
      "description": "Show multi-AI conversation and consensus",
      "suggestedComponents": ["AIConversationPanel", "AIPerspective", "ConsensusSummary"]
    },
    "capability-discovery": {
      "description": "Show available capabilities",
      "suggestedComponents": ["CapabilityGrid", "CapabilityCard"]
    }
  }
}
